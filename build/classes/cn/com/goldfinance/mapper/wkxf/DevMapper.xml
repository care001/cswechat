<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.goldfinance.mapper.wkxf.DevMapper">

	
	<select id="getAllDev"  resultType="cn.com.goldfinance.domain.wkxf.Dev">
	
	select
	    dev.devId as devId,
	    dev.devName as devName
	from Com_Dev dev
	</select>
</mapper>