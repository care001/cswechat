 <script src="../js/jquery-1.9.1.min.js" type="text/javascript"></script>
 <script src="../js/jquery.min.js" type="text/javascript"></script>    
 <script src="../js/jQuery-jcDate.js" type="text/javascript"></script>   
 <script type="text/javascript">
  //限制起止时间不能为空，开始时间不能大于结束时间
function check(){
	if($("#start").val()==""){
		var errorMsg = '开始时间不能为空！';
		$(".starterr").text(errorMsg);
		return false;
	}else{
		$(".starterr").text("");
	}
	 if($("#end").val()==""){
		var errorMsg = '结束时间不能为空！';
		$(".enderr").text(errorMsg);
		return false;
	}else{
		$(".enderr").text("");
	}
	var startime=$("#start").val();
	var endtime=$("#end").val();
	var arrs = startime.split("-");
	var arre = endtime.split("-");
	startime = new Date(arrs[0],arrs[1],arrs[2]); 
    endtime = new Date(arre[0],arre[1],arre[2]); 
		if (startime<=endtime) { 
			return  true; 
	    }else{
			var errorMsg = '开始时间不能大于结束时间！';
			$(".starterr").text(errorMsg);
			return  false; 
	    }
}
 $(document).ready(function(){
 	var url=window.location .href;
	var aa=url.indexOf("=");
	if (aa == -1){
		url="";
	}else{
		url=url.substring(aa+1);
	}
	$("#showuserid").val(url);
	$.ajax({ 
         type: "post", 
                cache: false, 
                dataType: 'json',
                url: "../admin/ShowInfoByMyself.ext",
                data:{
                	userid:url
                 },
				success: function(msg) {
                  $("#showname").text(msg.user.username);
                  $("#showaddress").text(msg.user.address);
                  $("#showphone").text(msg.user.phone);
                },
                error: function(data){
                    alert("网络错误或未登陆,请重新登录");
                }       
           });
           
           
 $("input.jcDate").jcDate({					       
			IcoClass : "jcDateIco",
			Event : "click",
			Speed : 100,
			Left : 0,
			Top : 28,
			format : "-",
			Timeout : 100
	});
 


 $("#search").click(function(){
 	if(check()){
 	var startTime = $("#start").val();
	var endTime = $("#end").val();
	var start= gettime(startTime);
	var end= gettime(endTime);
	var userid=$("#showuserid").val();
	var cardsel=$("#cardsel").val();
  	$.ajax({ 
         type: "post", 
                cache: false, 
                dataType: 'json',
                url: "../admin/GetConHis.ext",
                data:{
					startdate:start,
					enddate:end,
					userid:userid,
					pagenumstr:1,
					"cardsel":cardsel
                 },
				success: function(msg) {
                   if (msg.pagesum=='0') {
                   		$("#histbody").find("tr").remove();
                      	$("#histbody").append("<tr><td colspan='7'>没有查到数据</td></tr>");
                      	$("#pagesum").html(msg.pagesum);
                      	$("#pageall").html(msg.pageall);
                      	$("#pagenow").html(msg.pagenow);
                   } else if (msg.pagesum=='err') {
                   		alert(msg.desc);
                      
                   }else {
                   		$("#histbody").find("tr").remove();
                  		for(var i=0;i<msg.consumes.length;i++){
                  			var walletstr="未知";
                  			if(msg.consumes[i].wallettype=="7"){
                  				walletstr="ID金诚币";
                  			}else if(msg.consumes[i].wallettype=="8"){
                  				walletstr="ID现金";
                  			}else if(msg.consumes[i].wallettype=="5"){
                  				walletstr="IC金诚币";
                  			}else if(msg.consumes[i].wallettype=="6"){
                  				walletstr="IC现金";
                  			}else{
                  				walletstr="未知";
                  			}
                  			$("#histbody").append("<tr><td>"+(i+1)+"</td><td>"+msg.consumes[i].maketime+"</td><td>"+msg.consumes[i].clientid+"</td><td>"+msg.consumes[i].clientname+"</td><td>￥"+msg.consumes[i].cssum+"</td><td>"+walletstr+"</td><td>"+msg.consumes[i].csmerchat+"</td></tr>");
                   			
                   		}
                      	$("#pagesum").html(msg.pagesum);
                      	$("#pageall").html(msg.pageall);
                      	$("#pagenow").html(msg.pagenow);
                   }
                },
                error: function(data){
                    alert("网络错误或未登陆,请重新登录");
                }       
           });
 	}
 	
 });



function post(URL, PARAMS) {
	var temp = document.createElement("form");
	temp.action = URL;
	temp.method = "post";
	temp.style.display = "none";
	for (var x in PARAMS) {
		var opt = document.createElement("textarea");
		opt.name = x;
		opt.value = PARAMS[x];
		temp.appendChild(opt);
	}
	document.body.appendChild(temp);
	temp.submit();
	}

$("#excel").click(function(){
	if(check()){
	var startTime = $("#start").val();
	var endTime = $("#end").val();
	var start= gettime(startTime);
	var end= gettime(endTime);
	var cardsel=$("#cardsel").val();
	post('../admin/BuildExcel.ext', {startdate:start,enddate:end,userid:$("#showuserid").val(),cardsel:cardsel});
	}
	
});
//首页
$("#firstbut").click(function(){
	dosearch(1);
});

//上一页
$("#upbut").click(function(){
	var pagenow=$("#pagenow").html();
	if(pagenow==1){
		alert("已经是第一页了");
	}else{
		dosearch(Number(pagenow)-1);
	}
	
});

//下一页
$("#downbut").click(function(){
	var pagenow=$("#pagenow").html();
	var pageall=$("#pageall").html();
	if(pagenow==pageall){
		alert("已经是最后一页了");
	}else{
		dosearch(Number(pagenow)+1);
	}
});

//尾页
$("#lastbut").click(function(){
	var pageall=$("#pageall").html();
	dosearch(Number(pageall));

});

